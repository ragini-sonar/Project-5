<table class="table table-striped table-condensed mt-5">
    <tbody class="movie_id" id={{{movieId}}}></tbody>
</table>

<div class="stars" align="center" style="background: #ccc; padding:10px;">
    <i id="s1" class="fas fa-star fa-2x" data-index="1"></i>
    <i id="s2" class="fas fa-star fa-2x" data-index="2"></i>
    <i id="s3" class="fas fa-star fa-2x" data-index="3"></i>
    <i id="s4" class="fas fa-star fa-2x" data-index="4"></i>
    <i id="s5" class="fas fa-star fa-2x" data-index="5"></i><br>
</div>
<div id="detail-container" align="center" class="bg-secondary text-white"></div>
<br>
<div class="col text-center">
    <button type="submit" class="btn btn-dark" align="center" id="submit">Submit</button>
</div>

